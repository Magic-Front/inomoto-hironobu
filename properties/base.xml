<template id="ogp-template" xmlns:yc="urn:yaohata-components">
<div class="card" style="max-width:32rem;margin:1rem auto;">
<img yc:image="" />
<div class="card-body">
<h5 class="card-title"><yc:title /></h5>
<p class="card-text"><yc:description/></p>
<a yc:path="" class="btn btn-primary">ページを見る</a>
</div>
</div>
</template>

<!-- ロード中表示テンプレート -->
<template id="loading-template" xmlns:yc="urn:yaohata-components">
<div class="spinner-border" role="status">
<span class="sr-only">Loading...</span>
</div>
</template>

<!-- ロードエラーテンプレート -->
<template id="loaderror-template">
<div class="card" style="max-width:32rem;margin:1rem auto;">
<div class="card-body">
<div class="text-danger">
error
</div>
</div>
</div>
</template>

<template id="link-template" xmlns:yc="urn:yaohata-components">
<div><a yc:href="{path}"><yc:title/></a></div>
</template>

<template id="quote-template" xmlns:yc="urn:yaohata-components">
<blockquote class="internal-quote">
<div class="target"><yc:content/></div>
<div class="citation"><cite><a yc:href="{path}"><yc:title/></a></cite></div>
</blockquote>
</template>

<template id="twitter" xmlns:yc="urn:yaohata-components">
<div class="btn" style="border: solid 2px #55acee">
<a class="share" yc:href="https://twitter.com/share?url={url}&amp;text={title}" rel="nofollow noopener" target="_blank"><i class="fab fa-twitter">twitter</i></a>
</div>
</template>

<template id="facebook" xmlns:yc="urn:yaohata-components">
<div class="btn" style="border: solid 2px #3b5998">
<a class="share" yc:href="http://www.facebook.com/share.php?u={url}" rel="nofollow noopener" target="_blank"><i class="fab fa-facebook">facebook</i></a>
</div>
</template>

<template id="linkedin" xmlns:yc="urn:yaohata-components">
<div class="btn" style="border: solid 2px #0a66c2">
<a class="share" yc:href="https://www.linkedin.com/sharing/share-offsite/?url={url}" rel="nofollow noopener" target="_blank"><i class="fab fa-linkedin">linkedin</i></a>
</div>
</template>

